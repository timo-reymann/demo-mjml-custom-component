{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "incremental": true,
    "target": "es2015",
    "module": "commonjs",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true
  }
}
